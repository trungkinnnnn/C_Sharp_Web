@model IEnumerable<bai_tap_lon_web_trang_suc.Models.ChiTietPhieuNhap>

<table class="table">
    <tr>
        <th>
            Phiếu nhập
        </th>
        <th>
            Tên sản phẩm
        </th>
        <th>
            Đơn giá
        </th>
        <th>
            Số lượng
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @item.ID_PhieuNhap
            </td>
            <td>
                @item.SanPham.TenSanPham
            </td>
            <td>
                @item.DonGia
            </td>
            <td>
                @item.SoLuong
            </td>
        </tr>
    }

</table>
